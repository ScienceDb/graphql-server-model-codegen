{
    "model": "author",
    "storageType": "zendro-server",
    "url": "http://server1:3000/graphql",
    "attributes": {
        "id": "String",
        "name": "String",
        "lastname": "String",
        "email": "String",
        "book_ids": "[ String ]"
    },

    "associations":{
      "books":{
        "type": "many_to_many",
        "implementation": "foreignkey",
        "target": "book",
        "targetKey": "author_ids",
        "sourceKey": "book_ids",
        "keyIn": "author",
        "targetStorageType": "zendro-server"
      }
    },
    "internalId": "id"
  }
