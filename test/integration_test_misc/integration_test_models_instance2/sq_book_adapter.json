{
    "model": "sq_book",
    "storageType": "sql-adapter",
    "adapterName": "book_local",
    "regex": "local",
    "attributes": {
        "id": "String",
        "title": "String",
        "genre": "String",
        "ISBN": "String",
        "author_ids": "[ String]"
    },

    "associations":{
      "authors":{
        "type": "many_to_many",
        "implementation": "foreignkey",
        "target": "sq_author",
        "targetKey": "book_ids",
        "sourceKey": "author_ids",
        "keysIn": "sq_book",
        "targetStorageType": "distributed-data-model"
      }
    },

    "internalId": "id"
  }
