{
    "model": "book",
    "storageType": "zendro-server",
    "url": "http://server1:3000/graphql",
    "attributes": {
        "id": "String",
        "title": "String",
        "genre": "String",
        "ISBN": "String",
        "author_ids": "[ String]"
    },

    "associations":{
      "authors":{
        "type": "many_to_many",
        "implementation": "foreignkey",
        "target": "author",
        "targetKey": "book_ids",
        "sourceKey": "author_ids",
        "keysIn": "book",
        "targetStorageType": "zendro-server"
      }
    },

    "internalId": "id"
  }
