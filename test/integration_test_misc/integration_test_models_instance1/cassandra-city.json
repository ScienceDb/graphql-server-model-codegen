{
  "model" : "city",
  "storageType" : "cassandra",
  "attributes" : {
    "city_id": "String",
    "name": "String",

    "intArr": "[Int]",
    "strArr": "[String]",
    "floatArr": "[Float]",
    "boolArr": "[Boolean]",
    "dateTimeArr": "[DateTime]",

    "river_ids": "[String]"
  },
  "associations": {
    "rivers": {
      "type": "to_many",
      "target": "river",
      "targetStorageType": "sql",
      "sourceKey": "river_ids",
      "targetKey": "city_ids",
      "keyIn": "city",
      "reverseAssociationType":"to_many"
    }
  },
  "internalId": "city_id",
  "useDataLoader": true
}