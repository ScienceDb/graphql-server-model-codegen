{
  "model": "river",
  "storageType": "SQL",
  "attributes": {
    "name": "String",
    "length": "Int",
    "river_id": "String",
    
    "city_ids": "[String]"
  },
  "associations": {
    "countries": {
      "type": "many_to_many",
      "implementation": "sql_cross_table",
      "target": "country",
      "sourceKey": "river_id",
      "targetKey": "country_id",
      "keysIn": "country_to_river",
      "targetStorageType": "sql"
    },
    "cities": {
      "type": "many_to_many",
      "implementation": "foreignkey",
      "target": "city",
      "targetStorageType": "cassandra",
      "sourceKey": "city_ids",
      "targetKey": "river_ids",
      "keysIn": "river"
    }
  },
  "internalId": "river_id"
}